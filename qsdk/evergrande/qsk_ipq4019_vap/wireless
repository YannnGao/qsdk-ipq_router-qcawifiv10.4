config wifi-device  wifi0
	option type	qcawifi
	option channel	auto
	option macaddr	00:03:7f:12:ff:ff
	option hwmode	11ng
	# REMOVE THIS LINE TO ENABLE WIFI:
#	option disabled 1

config wifi-iface
	option device	wifi0
	option network	lan
	option mode	ap
	option ssid	OpenWrt2.4G
	option encryption none
config wifi-iface
	option device	wifi0
	option network	guest_lan
	option mode	ap
	option ssid	Evergrande-guest
	option encryption none
config wifi-iface
	option device	wifi0
	option network	subdev_lan
	option mode	ap
	option ssid	Evergrande-subdev
	option hidden '1'
	option encryption psk2
	option key 87654321

config wifi-device  wifi1
	option type	qcawifi
	option channel	auto
	option macaddr	00:34:56:78:a0:a0
	option hwmode	11ac
	# REMOVE THIS LINE TO ENABLE WIFI:
#	option disabled 1

config wifi-iface
	option device	wifi1
	option network	lan
	option mode	ap
	option ssid	OpenWrt5G
	option encryption none

config wifi-device  wifi2
	option type	qcawifi
	option channel	auto
	option macaddr	8c:fd:f0:07:34:46
	option hwmode	11ac
	# REMOVE THIS LINE TO ENABLE WIFI:
	option disabled 1

config wifi-iface
	option device	wifi2
	option network	lan
	option mode	ap
	option ssid	OpenWrt
	option encryption none

